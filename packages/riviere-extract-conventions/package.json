{
  "name": "@living-architecture/riviere-extract-conventions",
  "version": "0.2.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@living-architecture/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./default-config": "./src/default-extraction.config.json",
    "./eslint-plugin": {
      "types": "./src/eslint/index.d.ts",
      "require": "./src/eslint/index.cjs",
      "default": "./src/eslint/index.cjs"
    }
  },
  "files": [
    "dist",
    "src/default-extraction.config.json",
    "src/eslint/*.cjs",
    "src/eslint/*.d.ts",
    "src/eslint/*.d.cts",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@living-architecture/riviere-extract-config": "workspace:*"
  }
}
