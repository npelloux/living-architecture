      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@living-architecture/riviere-extract-config": "workspace:*",
    "minimatch": "^10.0.1",
    "ts-morph": "^24.0.0"
  }
}
