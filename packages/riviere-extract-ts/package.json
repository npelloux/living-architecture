{
  "name": "@living-architecture/riviere-extract-ts",
  "version": "0.1.2",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@living-architecture/source": "./src/index.ts",
